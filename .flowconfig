[ignore]
./webpack.config.js

[include]
src/**/*.jsx
flow-typed/**/*

[libs]

[lints]

[options]
module.name_mapper='models' -> '<PROJECT_ROOT>/src/models/index.js'
module.name_mapper='components' -> '<PROJECT_ROOT>/src/app/components'
