{
  "name": "respectful-heart",
  "version": "0.1.0",
  "description": "Webapp for Respectful Heart Counseling, LLC",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && webpack -p",
    "flow": "flow check",
    "flow-typed": "flow-typed",
    "flow-coverage": "flow-coverage-report",
    "lint": "standard --verbose | snazzy",
    "start": "webpack-dev-server --env local"
  },
  "license": "MIT",
  "devDependencies": {
    "axios": "0.17.1",
    "babel-cli": "6.26.0",
    "babel-core": "6",
    "babel-eslint": "7.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "body-parser": "^1.18.2",
    "caniuse-lite": "1.0.30000810",
    "cors": "2.8.4",
    "css-loader": "0.28.9",
    "css-module-flow": "1.0.0",
    "eslint": "4.17.0",
    "eslint-plugin-flowtype": "2.42.0",
    "file-loader": "1.1.7",
    "flow-bin": "0.65.0",
    "flow-coverage-report": "0.4.1",
    "flow-typed": "2.3.0",
    "handlebars": "4.0.11",
    "handlebars-loader": "1.6.0",
    "html-webpack-plugin": "2.30.1",
    "json-schema-faker": "0.5.0-rc13",
    "json-server": "0.12.1",
    "jsonschema": "1.2.2",
    "postcss": "6.0.19",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-promise": "0.5.3",
    "rimraf": "^2.6.2",
    "snazzy": "7.0.0",
    "standard": "10.0.3",
    "style-loader": "0.20.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "yargs": "11.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "flow-typed/"
    ],
    "globals": [
      "Class",
      "$Keys",
      "$Values",
      "$ObjMap",
      "ReducerReturn"
    ]
  },
  "flow-coverage-report": {
    "includeGlob": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "type": [
      "text",
      "html"
    ],
    "threshold": 100
  },
  "echelon": {
    "build": "npm run build",
    "publicContent": "./build"
  }
}
